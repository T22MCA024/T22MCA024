<!SELECTION SORT

#include<stdio.h>
main()
{
int a[100],temp,i,j,n,small;
printf("Enter n value:\n");
scanf("%d",&n);
printf("Enter array elements:\n");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
for(i=0;i<n;i++)
{
small=i;
for(j=i+1;j<n;j++)
{
if(a[j]<a[small])
{
temp=a[small];
a[small]=a[j];
a[j]=temp;
}
}
}
printf("Sorted array is :\n");
for(i=0;i<n;i++)
{
printf("a[%d]=%d\n",i,a[i]);
}
}
