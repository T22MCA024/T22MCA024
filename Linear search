
#include<stdio.h>
int main()
{
	int a[100],i,n,ele,data;
	printf("enter n values");
	scanf("%d",&n);
	printf("enter element");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("enter data\n");
	scanf("%d",&data);
	for(i=0;i<n;i++)
	{
		if(a[i]==data)
	     {
	     	printf("element found at index:%d",i);
	                          break;
	     }
	 }
	 if(i==n)
      {
	     	printf("element not found");
	  }
	     
}    
	
